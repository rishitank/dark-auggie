{
  "name": "@tankster-ai/n8n-nodes-dark-auggie",
  "version": "0.1.0",
  "description": "Dark Auggie: n8n community node wrapping the Augment Code Auggie CLI for automated AI workflows.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24.5.0"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "n8n",
    "n8n-node",
    "auggie",
    "augment",
    "augmentcode",
    "ai",
    "automation"
  ],
  "author": "tankster-ai",
  "n8n": {
    "nodes": [
      "dist/nodes/DarkAuggie/DarkAuggie.node.js"
    ],
    "credentials": [
      "dist/credentials/AugmentApi.credentials.js"
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "changelog:ai": "node scripts/generate-ai-changelog.mjs",
    "changelog:ai:since": "node scripts/generate-ai-changelog.mjs --since '2 weeks ago'"
  },
  "dependencies": {
    "@augmentcode/auggie": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "typescript": "^5.9.2"
  }
}
